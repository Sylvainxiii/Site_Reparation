{% extends 'base.html.twig' %}

{% block title %}FrInterventionInt index{% endblock %}

{% block body %}
    <h1>FrInterventionInt index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Int_type</th>
                <th>Int_date_debut</th>
                <th>Int_date_fin</th>
                <th>Int_numero_de_serie</th>
                <th>Int_difficulte</th>
                <th>Int_description</th>
                <th>Int_visibilite</th>
                <th>Int_date_add</th>
                <th>Int_date_edit</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for fr_intervention_int in fr_intervention_ints %}
            <tr>
                <td>{{ fr_intervention_int.id }}</td>
                <td>{{ fr_intervention_int.intType }}</td>
                <td>{{ fr_intervention_int.intDateDebut ? fr_intervention_int.intDateDebut|date('Y-m-d') : '' }}</td>
                <td>{{ fr_intervention_int.intDateFin ? fr_intervention_int.intDateFin|date('Y-m-d') : '' }}</td>
                <td>{{ fr_intervention_int.intNumeroDeSerie }}</td>
                <td>{{ fr_intervention_int.intDifficulte }}</td>
                <td>{{ fr_intervention_int.intDescription }}</td>
                <td>{{ fr_intervention_int.intVisibilite }}</td>
                <td>{{ fr_intervention_int.intDateAdd ? fr_intervention_int.intDateAdd|date('Y-m-d') : '' }}</td>
                <td>{{ fr_intervention_int.intDateEdit ? fr_intervention_int.intDateEdit|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('app_fr_intervention_int_show', {'id': fr_intervention_int.id}) }}">show</a>
                    <a href="{{ path('app_fr_intervention_int_edit', {'id': fr_intervention_int.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="11">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_fr_intervention_int_new') }}">Create new</a>
{% endblock %}
